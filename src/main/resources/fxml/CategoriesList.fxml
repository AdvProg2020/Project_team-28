<?import com.jfoenix.controls.JFXButton?>
<StackPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="graphics.CategoriesList" prefHeight="400.0" prefWidth="300.0">
