<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.jfoenix.controls.JFXTreeTableView?>
<?import com.jfoenix.controls.JFXButton?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="graphics.Cart" prefHeight="400.0" prefWidth="600.0" fx:id="mainPane">
    <JFXTreeTableView fx:id="tableView"/>
    <HBox alignment="CENTER" spacing="20">
        <JFXButton text="Close cart" onAction="#closeCart" styleClass="handleButton"/>
        <JFXButton text="Go to Payment page" onAction="#goToPaymentPage" styleClass="handleButton"/>
    </HBox>


</VBox>
